<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hangxin.persistence.mapper.PeopleMapper">

	<insert id="peopleInsertXml" parameterType="java.util.Map">
		INSERT people (name, 
					   sex, 
					   age, 
					   height, 
					   weight)
			   VALUES (#{name}, 
			   		   #{sex}, 
			   		   #{age}, 
			   		   #{height}, 
			   		   #{weight});
	</insert>

</mapper>